{
    "name": "chriscmiller/low6-tech-test",
    "type": "project",
    "description": "Technical Test For Low6",
    "keywords": ["application", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/horizon": "^5.7",
        "laravel/tinker": "^2.5"
    },
    "require-dev": {
        "enlightn/enlightn": "^1.22",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nikic/php-parser": "^4.12",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.11",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/phpstan-strict-rules": "^0.12.10",
        "phpunit/phpunit": "^9.3.3",
        "slevomat/coding-standard": "^7.0",
        "symplify/easy-coding-standard": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": [
            "vendor/bin/parallel-lint ./ --blame --exclude vendor"
        ],
        "ecs": [
            "vendor/bin/ecs check"
        ],
        "static": [
            "vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "analyse": [
            "php artisan enlightn --details --ci"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan cache:clear",
            "php artisan test"
        ],
        "before-push": [
            "vendor/bin/parallel-lint ./ --blame --exclude vendor",
            "vendor/bin/ecs check --clear-cache",
            "vendor/bin/phpstan analyse --memory-limit=2G",
            "php artisan enlightn --details --ci",
            "Composer\\Config::disableProcessTimeout",
            "php artisan cache:clear",
            "php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
